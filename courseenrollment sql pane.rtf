\f0\fs24 \cf0 -- Table: courseenrollment\
\
-- DROP TABLE courseenrollment;\
\
CREATE TABLE courseenrollment\
(\
  id text,\
  "number" text,\
  sectionid integer,\
  unit integer,\
  entry integer NOT NULL,\
  CONSTRAINT pk_entry PRIMARY KEY (entry),\
  CONSTRAINT fk_number FOREIGN KEY ("number")\
      REFERENCES course ("number") MATCH SIMPLE\
      ON UPDATE NO ACTION ON DELETE NO ACTION,\
  CONSTRAINT fk_sectionid FOREIGN KEY (sectionid)\
      REFERENCES class (sectionid) MATCH SIMPLE\
      ON UPDATE NO ACTION ON DELETE NO ACTION,\
  CONSTRAINT fk_studentid FOREIGN KEY (id)\
      REFERENCES student (id) MATCH SIMPLE\
      ON UPDATE NO ACTION ON DELETE NO ACTION\
)\
WITH (\
  OIDS=FALSE\
);\
ALTER TABLE courseenrollment\
  OWNER TO postgres;\
\
-- Index: fki_number\
\
-- DROP INDEX fki_number;\
\
CREATE INDEX fki_number\
  ON courseenrollment\
  USING btree\
  (number COLLATE pg_catalog."default");\
\
-- Index: fki_sectionid\
\
-- DROP INDEX fki_sectionid;\
\
CREATE INDEX fki_sectionid\
  ON courseenrollment\
  USING btree\
  (sectionid);\
\
-- Index: fki_studentid\
\
-- DROP INDEX fki_studentid;\
\
CREATE INDEX fki_studentid\
  ON courseenrollment\
  USING btree\
  (id COLLATE pg_catalog."default");\
